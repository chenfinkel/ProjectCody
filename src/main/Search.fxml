<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.View">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <TextField fx:id="searchlName" disable="true" GridPane.columnIndex="2" />
        <Label text="Username" GridPane.rowIndex="1" />
        <Label text="First Name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Label text="Last Name" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Label text="Birthdate" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Label text="City" GridPane.columnIndex="4" GridPane.rowIndex="1" />
        <TextField fx:id="searchUser" disable="true" />
        <TextField fx:id="searchfName" disable="true" GridPane.columnIndex="1" />
        <TextField fx:id="searchbDate" disable="true" GridPane.columnIndex="3" />
        <TextField fx:id="searchCity" disable="true" GridPane.columnIndex="4" />
        <Button mnemonicParsing="false" onAction="#returnUserPage" text="Back" GridPane.rowIndex="2" />
    </children>
</GridPane>
